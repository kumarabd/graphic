type Query {
  listNodes(filter: StringMap!): [Node!]!
  listEdges(filter: StringMap!): [Edge!]!
}

type Mutation {
  updateNode(id: String, node: Node!): Status!
  updateEdge(id: String, edge: Edge!): Status!
}

type Subscription {
  watchNodes(filter: StringMap!): [Node!]!
  watchEdges(filter: StringMap!): [Edge!]!
}

scalar StringMap
scalar Status
scalar Node
scalar Edge